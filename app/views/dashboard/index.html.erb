<div id="container">
  <nav>
    <div class="menu">
      <img src="assets/logo.png" title="na bronk"></img>
      <p>
        <a href="#">¿qué?</a>
      </p>
    </div>
  </nav>
  <div class="stream">
    <% unless @thangs.blank? %>
      <% @thangs.each do |thang| %>
      <div class="kool">
        <img class="flyer" src="assets/flyer_<%= cycle('0.jpg', '1.png', '2.jpg') -%>" />
        <div class="meta">
          <div class="what">
            <h2><%= link_to thang.title, thang_path(thang) %></h2>
            <p><small><%= thang.when %> | <%= thang.where %></small></p>
          </div>
          <div class="description"></div>
        </div>
      </div>
      <% end %>
    <% end%>
  </div>
</div>
